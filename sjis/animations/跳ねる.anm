if(obj.time < obj.track0 * obj.track1) then
  time = (obj.time % obj.track0 - obj.track0 / 2) * 2
  ratio = 1 - (time / obj.track0) ^ 2
  obj.oy = obj.oy + obj.track2 * ratio
  obj.aspect = obj.track3 * ratio / 100
